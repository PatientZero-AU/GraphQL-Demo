  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "swagger": "nswag swagger2tsclient /classname:{controller}Client /Template:Angular /TypeScriptVersion:2.0 /ClientBaseClass:BaseClient /ConfigurationClass:BaseConfiguration /extensionCode:src/app/shared/api/base-client.service.ts /useTransformOptionsMethod:true /datetimetype:Date /generateclientclasses:true /generatedtotypes:true /operationgenerationmode:SingleClientFromOperationId /GenerateConstructorInterface:false /generatereadonlykeywords:true /input:http://localhost:54903/swagger/v1/swagger.json /serviceschemes:http /output:src/app/shared/api/client.service.ts",
    "graphql:schema:download": "apollo-codegen introspect-schema http://localhost:54903/api/graphql --output src/app/shared/api/graphql/schema.json",
    "graphql:schema:generate": "apollo-codegen generate src/app/pages/**/*.gql --schema src/app/shared/api/graphql/schema.json --target typescript --output src/app/shared/api/graphql/schema.ts",
    "graphql": "npm run graphql:schema:download && npm run graphql:schema:generate",
    "watch:prod": "npm run build:prod -- --watch=true",
    "watch:prod-aot": "npm run watch:prod --aot=true",
    "watch:dev": "npm run build:dev -- --watch=true",
    "watch:dev-aot": "npm run watch:dev --aot=true",
    "build:prod": "ng build --target=production --environment=prod",
    "build:prod-aot": "npm run build:prod -- --aot=true",
    "build:dev": "ng build --target=development --environment=dev",
    "build:dev-aot": "npm run build:dev -- --aot=true"
  },
